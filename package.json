{
    "name": "youzan-sdk",
    "version": "0.0.8",
    "description": "Yet Another Node.js SDK for http://open.youzan.com",
    "main": "index.js",
    "scripts": {
        "test": "ava test/test_*.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rayqian/youzan-sdk.git"
    },
    "keywords": [
        "koudaitong",
        "youzan",
        "sdk",
        "youzan-sdk",
        "有赞",
        "口袋通"
    ],
    "author": "Ray",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/rayqian/youzan-sdk/issues"
    },
    "homepage": "https://github.com/rayqian/youzan-sdk#readme",
    "dependencies": {
        "request": "^2.69.0"
    },
    "devDependencies": {
        "ava": "^0.13.0",
        "eslint": "^2.5.3",
        "eslint-plugin-ava": "^1.3.2"
    },
    "eslintConfig": {
        "env": {
            "es6": true
        },
        "parserOptions": {
            "ecmaVersion": 6,
            "sourceType": "module"
        },
        "plugins": [
            "ava"
        ],
        "rules": {
            "ava/max-asserts": [2, 5],
            "ava/no-cb-test": 0,
            "ava/no-identical-title": 2,
            "ava/no-invalid-end": 2,
            "ava/no-only-test": 2,
            "ava/no-skip-assert": 2,
            "ava/no-skip-test": 2,
            "ava/no-statement-after-end": 2,
            "ava/no-todo-test": 1,
            "ava/prefer-power-assert": 0,
            "ava/test-ended": 2,
            "ava/test-title": [2, "always"],
            "ava/use-t": 2,
            "ava/use-test": 2
        }
    }
}
